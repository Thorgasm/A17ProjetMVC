@model IEnumerable<A17ProjetMVC.Models.Objet>

@{
    ViewBag.Title = "Mes Objets";
}

<h2>MyObjects</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nomObjet)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nomObjet)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ObjetID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ObjetID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ObjetID })
        </td>
    </tr>
}

</table>
